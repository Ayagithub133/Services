<Microsoft.AspNetCore.Components.Authorization.CascadingAuthenticationState>
<Microsoft.AspNetCore.Components.Routing.Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    
    
    <Found Context="routeData">
        <Microsoft.AspNetCore.Components.RouteView RouteData="@routeData" DefaultLayout="@typeof(Services.Client.Shared.MainLayout)" />
    </Found>

    <NotFound>
        <Microsoft.AspNetCore.Components.LayoutView Layout="@typeof(Services.Client.Shared.MainLayout)">
            
        </Microsoft.AspNetCore.Components.LayoutView>
    </NotFound>
   
</Microsoft.AspNetCore.Components.Routing.Router>
</Microsoft.AspNetCore.Components.Authorization.CascadingAuthenticationState>
